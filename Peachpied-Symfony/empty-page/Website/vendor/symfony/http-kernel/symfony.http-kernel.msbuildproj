<Project Sdk="Peachpie.NET.Sdk/0.9.35">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>.NET class library in PHP</Description>
    <AssemblyName>symfony.http-kernel</AssemblyName>
    <PackageId>symfony.http-kernel</PackageId>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**/*.php" Exclude="tests/**; DataCollector/DumpDataCollector.php; Client.php" />
    <Content Include="**" Exclude="**/*.php;obj/**;bin/**;*.msbuildproj" CopyToOutputDirectory="PreserveNewest">
      <PackagePath>contentFiles/any/netcoreapp2.0/symfony/</PackagePath>
      <PackageCopyToOutput>true</PackageCopyToOutput>
      <Link>symfony\%(RecursiveDir)%(Filename)%(Extension)</Link>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\psr\log\psr.log.msbuildproj" />
    <ProjectReference Include="..\event-dispatcher\symfony.event-dispatcher.msbuildproj" />
    <ProjectReference Include="..\http-foundation\symfony.http-foundation.msbuildproj" />
  </ItemGroup>
</Project>