<Project Sdk="Peachpie.NET.Sdk/0.9.35">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>.NET class library in PHP</Description>
    <AssemblyName>PhpLib1</AssemblyName>
    <PackageId>PhpLib1</PackageId>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**/*.php" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\psr\cache\psr-cache.msbuildproj" />
    <ProjectReference Include="..\..\psr\log\psr-log.msbuildproj" />
    <ProjectReference Include="..\..\psr\simple-cache\psr-simple-cache.msbuildproj" />
  </ItemGroup>
</Project>