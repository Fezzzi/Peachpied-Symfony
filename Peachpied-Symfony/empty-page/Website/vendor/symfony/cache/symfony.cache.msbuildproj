<Project Sdk="Peachpie.NET.Sdk/0.9.35">
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>.NET class library in PHP</Description>
    <AssemblyName>symfony.cache</AssemblyName>
    <PackageId>symfony.cache</PackageId>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**/*.php" Exclude="tests/**;DoctrineProvider.php" />
    <Content Include="**" Exclude="**/*.php;obj/**;bin/**;*.msbuildproj" CopyToOutputDirectory="PreserveNewest">
      <PackagePath>contentFiles/any/netcoreapp2.0/symfony/</PackagePath>
      <PackageCopyToOutput>true</PackageCopyToOutput>
      <Link>symfony\%(RecursiveDir)%(Filename)%(Extension)</Link>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\psr\log\psr.log.msbuildproj" />
    <ProjectReference Include="..\..\psr\simple-cache\psr.simple-cache.msbuildproj" />
    <ProjectReference Include="..\contracts\symfony.contracts.msbuildproj" />
    <ProjectReference Include="..\dependency-injection\symfony.dependency-injection.msbuildproj" />
    <ProjectReference Include="..\http-kernel\symfony.http-kernel.msbuildproj" />
  </ItemGroup>
</Project>