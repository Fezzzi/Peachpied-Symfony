{
    "name": "symfony/downloader",
    "type": "project",
    "license": "MIT",
    "description": "Application used to download and compile symfony components to .NET",
    "scripts": {
		"echo-dependencies": "@php compile-component.php",
        "post-package-install": [
            "@php compile-component.php"
        ]
    },
	"require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "*",
        "symfony/templating": "4.2.*",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "4.2.*",
        "symfony/validator": "4.2.*",
        "symfony/yaml": "*"
    },
	"conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    }
}
